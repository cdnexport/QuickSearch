{
    "name": "QuickTube",
    "version": "0.1",
    "description": "Search YouTube with one click.",
    "manifest_version": 2,

    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["<all_urls>", "contextMenus", "activeTab"],
    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["injectable.js"]
    }]
}
